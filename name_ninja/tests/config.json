{
    "name": "name_ninja_test",
    "author": "SirLich",
    "packs": {
        "behaviorPack": "./packs/BP",
        "resourcePack": "./packs/RP"
    },
    "regolith": {
        "profiles": {
            "dev": {
                "unsafe": false,
                "filters": [
                    {
                        "url": "github.com/SirLich/regolith-filters/name_ninja",
                        "settings": {
                            "language": "en_US.lang",
                            "overwrite": false,
                            "entities": {
                                "auto_name": true,
                                "prefix": "§1",
                                "postfix": "§r"
                            },
                            "blocks": {
                                "auto_name": true,
                                "prefix": "§2",
                                "postfix": "§r"
                            },
                            "items": {
                                "auto_name": true,
                                "prefix": "§3",
                                "postfix": "§r"
                            },
                            "spawn_eggs": {
                                "auto_name": true,
                                "prefix": "§4",
                                "postfix": " Spawn Egg§r"
                            }
                        }
                    }
                ],
                "export": {
                    "target": "development"
                },
                "dataPath": "./packs/data"
            },
            "local": {
                "unsafe": false,
                "filters": [
                    {
                        "runWith": "python",
                        "script": "../name_ninja.py",
                        "settings": {
                            "language": "en_GB.lang",
                            "overwrite": false,
                            "entities": {
                                "auto_name": true,
                                "prefix": "§1",
                                "postfix": "§r"
                            },
                            "blocks": {
                                "auto_name": true,
                                "prefix": "§2",
                                "postfix": "§r"
                            },
                            "items": {
                                "auto_name": true,
                                "prefix": "§3",
                                "postfix": "§r"
                            },
                            "spawn_eggs": {
                                "auto_name": true,
                                "prefix": "§4",
                                "postfix": " Spawn Egg§r"
                            }
                        }
                    }
                ],
                "export": {
                    "target": "local"
                },
                "dataPath": "./packs/data"
            }
        }
    }
}